{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": false,
        "module": "commonjs",
        "target": "es5",
        "jsx": "preserve",
        "lib": ["es5", "es6", "dom"],
    },
    "include": [
        "./src/**/*",
    ],
    "files": [
        "./src/index.d.ts"
    ],
    "awesomeTypescriptLoaderOptions": {
        "reportFiles": [
            "./src/**/*"
        ]
    },
}
