{
  "name": "docs",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:lib-install-locally": "npm-run-all dev:compile-js dev:compile-sass copy:json link ",
    "dev:watch-sass": "node-sass src/scss/future.scss future/css/future.css -w",
    "dev:compile-sass": "node-sass src/scss/future.scss future/css/future.css",
    "dev:compile-js": "babel src/js --out-dir future/js --source-maps inline",
    "dev:watch-js": "babel src/js --watch --out-dir future/js --source-maps inline",
    "copy:scss": "cp -r src/scss future/scss",
    "copy:json": "cp -r dist/package.json future/package.json",
    "link": "cd future && sudo npm link && cd .. && npm link future",
    "unlink": "cd future && sudo npm unlink",
    "prod:build-docs": "ng build",
    "prod:build-lib": "webpack -p",
    "clean": "rm -rf future"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/forms": "~9.1.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/router": "~9.1.0",
    "autoprefixer": "^9.7.6",
    "concat": "^1.0.3",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "node-sass": "^4.14.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "postcss-loader": "^3.0.0",
    "rxjs": "~6.5.4",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "tslib": "^1.10.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "~9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3",
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.7.0",
    "babel-loader": "^8.1.0",
    "@babel/preset-env": "^7.9.5",
    "jquery": "^3.5.0"
  }
}
