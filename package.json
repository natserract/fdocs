{
  "name": "docs",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:watch-sass": "node-sass projects/salt/lib/scss/salt.scss projects/salt/lib/css/main.css -w",
    "dev:compile-sass": "node-sass projects/salt/lib/scss/salt.scss projects/salt/lib/css/main.css",
    "copy:css": "cp -r projects/salt/dist/css/ dist/salt/lib/css",
    "prod:build-docs": "ng build --prod",
    "prod:build-webcomp": "ng build webcomponent --prod",
    "prod:build:lib": "npm-run-all clean:build build:lib dev:compile-sass prefix copy:css webpack:build prod:build-webcomp clean:dist",
    "prefix": "postcss projects/salt/lib/css/main.css --use=autoprefixer --map=false --output=projects/salt/dist/css/salt.css",
    "link": "cd dist/salt && sudo npm link && cd .. && cd .. && npm link salt",
    "webpack:build": "webpack -p",
    "build:lib": "ng build salt",
    "access": "sudo chown -R $USER /usr/local/lib/node_modules",
    "unlink": "cd dist/salt && sudo npm unlink && cd .. && cd .. && npm unlink && npm-run-all access",
    "clean:build": "rm -rf dist/salt",
    "clean:dist": "rm -rf projects/salt/dist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
